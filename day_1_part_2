frequency = []
x = 0

inputdata = open("input.txt", "r")

# create and populate list of all variables used to calculate frequencies
variables = []
for line in inputdata:
    variables.append(int(line))
	
done = "False"
while done == "False":
    for i in variables [:]:
        x = x + i
        if x in frequency:
            print(x)
            done = "True"
            break
        frequency.append(x)
