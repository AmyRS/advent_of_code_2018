import collections
from collections import Counter

inputdata = open("input_boxID.txt", "r")
boxid = []
for line in inputdata:
    x = line.strip('\n')
    boxid.append(x)
    
threes = 0
twos = 0

for string in boxid:
    letterCounts = collections.Counter(string)
    for letter in letterCounts:
        if letterCounts[letter] == 3:
            threes = threes + 1
            break
    for letter in letterCounts:
        if letterCounts[letter] == 2:
            twos = twos + 1
            break

print (threes * twos)
